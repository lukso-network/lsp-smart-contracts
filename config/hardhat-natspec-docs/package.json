{
  "name": "@lukso/hardhat-natspec-docs",
  "version": "1.0.0",
  "private": true,
  "description": "Hardhat v3 plugin for generating NatSpec documentation from smart contract comments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "LUKSO",
  "license": "Apache-2.0",
  "keywords": [
    "hardhat",
    "hardhat-plugin",
